datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model TeaParty {
  id    Int     @id @default(autoincrement())
  scheduledAt DateTime
}

model ConfirmedSub {
  id    Int     @id @default(autoincrement())
  sub   String  @unique
}
